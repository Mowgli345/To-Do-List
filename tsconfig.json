{
  "compilerOptions": {   
    "target": "es6",                                 
    "module": "ES2015",
    "rootDir": "src/", 
    "outDir": "docs",                              
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                              
    "skipLibCheck": true,
    "sourceMap": true,     
    "noImplicitAny": true,        
    "strictNullChecks": true,                          
  }
}
